{
    "name": "S11 Exploration",
    "routes": {
        "image": {
            "get": {
                "topic": "/camera/image_raw",
                "topic_type": "sensor_msgs/Image",
                "fields": {
                    "data": {
                        "name": "image",
                        "type": "image"
                    }
                }
            }
        },
        "pose": {
            "get": {
                "topic": "/amcl_pose",
                "topic_type": "geometry_msgs/PoseWithCovarianceStamped",
                "fields": {
                    "pose.pose.position.x": {
                        "name": "x"
                    },
                    "pose.pose.position.y": {
                        "name": "y"
                    },
                    "pose.pose.position.z": {
                        "name": "z"
                    }
                }
            }
        },
        "laser": {
            "get": {
                "topic": "/scan",
                "topic_type": "sensor_msgs/LaserScan",
                "fields": {
                    "ranges": {
                        "name": "range"
                    },
                    "range_min": {
                        "name": "rage"
                    }                    
                }
            }
        },
        "map": {
            "get": {
                "topic": "/benchbot_workspace/costmap/costmap",
                "topic_type": "nav_msgs/OccupancyGrid",
                "fields": {
                    "info": {},
                    "data": {}
                }
            }
        },
        "heading": {
            "get": {
                "callback": "core/heading"
            }
        },
        "forward": {
            "post": {
                "callback": "core/forward"
            }
        },
        "left": {
            "post": {
                "callback": "core/left"
            }
        },
        "right": {
            "post": {
                "callback": "core/right"
            }
        },
        "is_done": {
            "get": {
                "callback": "core/is_done"
            }
        }
    },
    "objectives": {
        "goal_1a": {
            "callback": "simple/at_position",
            "position": {"x": 2, "y": 4},
            "tolerance": 0.3
        },
        "goal_1b": {
            "callback": "simple/at_position",
            "position": {"x": 3, "y": 7},
            "tolerance": 0.3
        },
        "goal_2": {
            "callback": "simple/at_location",
            "location": "kitchen",
            "depends": "(goal_1a && goal_1b) || !goal_2 "
        },
        "goal_2-a": {
            "callback": "simple/detected_objects_at_marker",
            "marker": "april_23"
        },
        "goal_3": {
            "callback": "simple/detected_objects_at_location",
            "location": "living_room"
        }
    },
    "locations": {
        "kitchen": {
            "position": {"x": 3, "y": 7},
            "radius": 0.3
        },
        "lounge": {
            "bounds": [
                {"x": 3, "y": 7},
                {"x": 3, "y": 7},
                {"x": 3, "y": 7},
                {"x": 3, "y": 7}
            ]
        }
    },
    "markers": {
        "april_23": {
            "location": "kitchen"
        }
    },
    "items": [
        {
            "type": "duck",
            "marker": "april_23"
        },
        {
            "type": "toy_car",
            "location": "kitchen"
        }
    ]
}