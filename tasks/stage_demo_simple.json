{
  "name": "S11 Exploration",
  "robots": ["guiabot", "pepper"],
  "routes": {
      "image": {
          "get": {
              "topic": "/camera/image_raw",
              "topic_type": "sensor_msgs/Image",
              "fields": {
                  "data": {
                      "name": "image",
                      "type": "image"
                  }
              }
          }
      },
      "pose": {
          "get": {
              "topic": "/amcl_pose",
              "topic_type": "geometry_msgs/PoseWithCovarianceStamped",
              "fields": {
                  "pose.pose.position.x": {
                      "name": "x"
                  },
                  "pose.pose.position.y": {
                      "name": "y"
                  },
                  "pose.pose.position.z": {
                      "name": "z"
                  }
              }
          }
      },
      "location": {
        "get": {
          "callback": "core/current_location"
        }
      },
      "locations": {
        "get": {
          "callback": "core/get_locations_from_task"
        }
      },
      "next": {
          "post": {
              "callback": "core/goto_next",
              "locations": [
                  "location_1",
                  "location_2",
                  "location_3"
              ]
          }
      },
      "goto": {
          "post": {
              "callback": "core/goto"
          }
      },
      "is_done": {
          "get": {
              "callback": "core/is_done"
          }
      }
  },
  "locations": [
    "home",
    "location_1",
    "location_2",
    "location_3"
  ],
  "objectives": {
      "main": {
          "callback": "simple/equals",
          "expected": "location_1"
      }
  }
}
